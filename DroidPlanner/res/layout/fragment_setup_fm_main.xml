<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/configLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center" >

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:orientation="vertical"
        tools:ignore="UselessParent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginBottom="10dp"
            android:background="@color/ground_speed_label"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textViewPWM"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="10dp"
                android:layout_weight="5"
                android:gravity="center"
                android:text="PWM in: 0000"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#fff"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textViewPWMRange"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="10dp"
                android:layout_weight="5"
                android:text="PWM Range ( No Signal )"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#fff"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/layoutFM1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingRight="10dp" >

            <TextView
                android:id="@+id/textViewFM1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/setup_fm_mode1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#fff" />

            <Spinner
                android:id="@+id/spinnerFM1"
                style="@style/ModeSpinnerText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="6"
                android:entries="@array/FligthMode_CopterV3_1" />

            <CheckBox
                android:id="@+id/checkBoxFM1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setup_fm_simple"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#fff" />

            <CheckBox
                android:id="@+id/checkBoxFMS1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setup_fm_super"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#fff" />
        </LinearLayout>

        <LinearLayout
